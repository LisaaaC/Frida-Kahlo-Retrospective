paintings = ['The Two Fridas', 'My Dress Hangs Here, Tree of Hope', 'Self Portrait With Monkeys']

dates = [1939,1933,1946,1940]

pnd= list(zip(paintings, dates))
print(pnd)


paintings.append(('The Broken Column', 1994)) 
paintings.append(('The Wounded Deer', 1946)) 
paintings.append(('Me and My Doll', 1937)) 

print(paintings)

print("the number of paintings are", len(paintings))

audio_tour_number = range(1,len(paintings)+1)
print(list(audio_tour_number))

master_list = zip(paintings, audio_tour_number)

print(list(master_list))
